<% cache @concert do %>
<%= render :partial => "shared/concert_header", :locals => {:back_button => false} %>

<ul data-role="listview" data-inset="true" data-shadow="false" data-corners="false" id="concert-stream">
  <%= render partial: 'posts/post', collection: @concert.posts.published %>
</ul>
<% end %>

<script>
  mixpanel.identify("<%= Rails.env %>-<%= current_user.id %>");
  mixpanel.people.set({
    "$email": "<%= current_user.email %>",

    "name": "<%= current_user.name %>",
  });
</script>